{
  "name": "u-node",
  "version": "0.0.2",
  "description": "A library to compactly encode data which can be used in URL.",
  "main": "lib/u.js",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "build": "babel src -d lib",
    "dist": "webpack lib/u.js dist/u.js --output-library u --output-library-target umd",
