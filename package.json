  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "jsverify": "^0.8.4",
    "mocha": "^9.1.3",
    "webpack": "^1.12.11",
    "lodash": "^4.17.21"
  },
  "dependencies": {}
}
